    "sinon-chai": "^2.8.0",
    "style-loader": "^0.16.0",
    "stylint": "^1.5.9",
    "stylint-stylish": "^1.4.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  },
  "config": {
    "ghooks": {
      "pre-push": "npm run lint && npm run test"
    }
  }
}
