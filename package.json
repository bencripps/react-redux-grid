    "redux-devtools": "^3.3.1",
    "redux-mock-store": "^1.2.1",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "stylint": "^1.5.9",
    "stylint-stylish": "^1.4.0",
    "stylus": "^0.54.5",
